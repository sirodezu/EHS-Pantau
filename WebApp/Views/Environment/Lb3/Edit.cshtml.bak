                               <input type="hidden" id="jenis_limbah_dihasilkan_id" name="jenis_limbah_dihasilkan_id" value="@fieldModel.jenis_limbah_dihasilkan_id"  required data-required-msg="@ResxHelper.GetValue("Message","RequiredTextFormat").Replace("{0}",@ResxHelper.GetValue("ta_lb3","jenis_limbah_dihasilkan_id"))" />
                               <script>
                                   var jenis_limbah_dihasilkan_id_param = {item : {value:"id",text:"nama"},parent:[{ field: 'cat_id',opr:'=', value: '90' }], order:[]};
                                   var jenis_limbah_dihasilkan_id_ds = new kendo.data.DataSource({ transport: { read: { url: "@ViewBag.BaseUrl/Ref/LiteralData/LookupData", dataType: "json", data: jenis_limbah_dihasilkan_id_param, type: "POST" } } });
                                   var jenis_limbah_dihasilkan_id = $("#jenis_limbah_dihasilkan_id_select").kendoMultiSelect({ placeholder: "", autoClose: true, filter: 'contains', height: 200, autoBind: false, dataValueField: "value", dataTextField: "text", change: jenis_limbah_dihasilkan_id_onChange, autoBind: false, dataSource: jenis_limbah_dihasilkan_id_ds });
                                   $(document).ready(function () { $("#jenis_limbah_dihasilkan_id_select").data("kendoMultiSelect").value([@fieldModel.jenis_limbah_dihasilkan_id]); });
                                   function jenis_limbah_dihasilkan_id_onChange() {
                                       var dataItem = $("#jenis_limbah_dihasilkan_id_select").data("kendoMultiSelect").dataItems();
                                       var cb_val = '';
                                       var cb_text = '';
                                       for (i = 0; i < dataItem.length; i++) {
                                           cb_val += cb_val != '' ? ',' : ''; cb_val += dataItem[i].value;
                                           cb_text += cb_text != '' ? ',' : ''; cb_text += dataItem[i].text;
                                       }
                                       $("#jenis_limbah_dihasilkan_id").val(cb_val);
                                       $("#jenis_limbah_dihasilkan_nama").val(cb_text);
                                   }
                               </script>
                           </div>
                       </div>

