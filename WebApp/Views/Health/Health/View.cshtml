                </div>
            </div>
            <div class="row">
                <div class="col-lg-12">
                    @{
                        <br /><hr /><h4>GCU Result</h4>
                        <partial name="_list_health_gcu" />

                        <br /><hr /><h4>Sumber Resiko Penyakit Akibat Kerja</h4>
                        <partial name="_list_health_sakit_kerja" />

                        <br /><hr /><h4>Gangguan Fisik / Organ Tubuh</h4>
                        <partial name="_list_health_ganggu_fisik" />
                    }
                </div>
            </div>
        </div>
		<div class="panel-footer">
			<a id="close" class="k-button k-button-icontext" href="#"><i class="far fa-times-circle"></i>&nbsp;<span id="btnCloseText">@ResxHelper.GetValue("Message", "Close", "Tutup")</span></a>
		</div>
	</div>
</div>
<script>
    $("#close").kendoButton();
    $("#close").click(function (e) {
        close_self();
    });
    function onCloseDialog(e) { $('#dialog').html(''); }
    function closeDialog() { $("#dialog").data("kendoWindow").close(); }
    function close_self() { if (window.parent.$("#dialog_ta_health").data("kendoWindow")) { window.parent.$("#dialog_ta_health").data("kendoWindow").close(); } else { window.parent.$("#dialog").data("kendoWindow").close(); } }
</script>