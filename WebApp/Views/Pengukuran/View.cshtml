@using WebApp
@using Microsoft.AspNetCore.Http;
@{
    Layout = "~/Views/Shared/_LayoutNoHeader.cshtml";
    var fieldModel = ViewData["fieldModel"] as WebApp.Models.PengukuranModel.ViewModel;
	ViewData["Title"] = ViewData["TitleHeader"];
    ViewData["PageName"] = "pengukuran";
    ViewData["Heading"] = ViewData["TitleHeader"];
    ViewData["Category1"] = "";
    ViewData["Category2"] = "";
}
<div class="panel panel-form">
	<div class="panel-container">
		<div class="panel-heading">
			<div class="title-header">@ViewData["TitleHeader"]</div>
		</div>
		<div class="panel-content">
			<div class="row">
				<div class="col-lg-12">

                    <div id="form_item_id" class="form-group-none">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_id" class="form-item-view">@Html.Raw(fieldModel.id)</div>
                  </div>

                    <div id="form_item_ehs_area_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "ehs_area_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_ehs_area_id" class="form-item-view">@Html.Raw(fieldModel.ehs_area_id_text)</div>
                  </div>

                    <div id="form_item_ba_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "ba_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_ba_id" class="form-item-view">@Html.Raw(fieldModel.ba_id_text)</div>
                  </div>

                    <div id="form_item_pa_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "pa_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_pa_id" class="form-item-view">@Html.Raw(fieldModel.pa_id_text)</div>
                  </div>

                    <div id="form_item_psa_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "psa_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_psa_id" class="form-item-view">@Html.Raw(fieldModel.psa_id_text)</div>
                  </div>

                    <div id="form_item_jenis_pemeriksaan_pengujian_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "jenis_pemeriksaan_pengujian_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_jenis_pemeriksaan_pengujian_id" class="form-item-view">@Html.Raw(fieldModel.jenis_pemeriksaan_pengujian_id_text)</div>
                  </div>

                    <div id="form_item_keterangan_series_kode" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "keterangan_series_kode")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_keterangan_series_kode" class="form-item-view">@Html.Raw(fieldModel.keterangan_series_kode)</div>
                  </div>

                    <div id="form_item_jumlah_titik_penaatan" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "jumlah_titik_penaatan")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_jumlah_titik_penaatan" class="form-item-view">@Html.Raw(fieldModel.jumlah_titik_penaatan)</div>
                  </div>

                    <div id="form_item_periode_pemeriksaan_pengujian_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "periode_pemeriksaan_pengujian_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_periode_pemeriksaan_pengujian_id" class="form-item-view">@Html.Raw(fieldModel.periode_pemeriksaan_pengujian_id_text)</div>
                  </div>

                    <div id="form_item_status_penaatan_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "status_penaatan_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_status_penaatan_id" class="form-item-view">@Html.Raw(fieldModel.status_penaatan_id_text)</div>
                  </div>

                    <div id="form_item_hasil_pengukuran" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "hasil_pengukuran")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_hasil_pengukuran" class="form-item-view">@Html.Raw(fieldModel.hasil_pengukuran)</div>
                  </div>

                    <div id="form_item_tanggal_pengujian" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "tanggal_pengujian")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_tanggal_pengujian" class="form-item-view">@Html.Raw(fieldModel.tanggal_pengujian)</div>
