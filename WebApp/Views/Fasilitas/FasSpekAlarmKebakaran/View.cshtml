@using WebApp
@using Microsoft.AspNetCore.Http;
@{
    Layout = "~/Views/Shared/_LayoutNoHeader.cshtml";
    var fieldModel = ViewData["fieldModel"] as WebApp.Models.FasSpekAlarmKebakaranModel.ViewModel;
	ViewData["Title"] = ViewData["TitleHeader"];
    ViewData["PageName"] = "fasspekalarmkebakaran";
    ViewData["Heading"] = ViewData["TitleHeader"];
    ViewData["Category1"] = "";
    ViewData["Category2"] = "";
}
<div class="panel panel-form">
	<div class="panel-container">
		<div class="panel-heading">
			<div class="title-header">@ViewData["TitleHeader"]</div>
		</div>
		<div class="panel-content">
			<div class="row">
				<div class="col-lg-12">

                    <div id="form_item_id" class="form-group-none">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_id" class="form-item-view">@Html.Raw(fieldModel.id)</div>
                  </div>

                    <div id="form_item_fasilitas_id" class="form-group-none">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "fasilitas_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_fasilitas_id" class="form-item-view">@Html.Raw(fieldModel.fasilitas_id)</div>
                  </div>

                    <div id="form_item_nomor_aset" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "nomor_aset")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_nomor_aset" class="form-item-view">@Html.Raw(fieldModel.nomor_aset)</div>
                  </div>

                    <div id="form_item_merk" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "merk")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_merk" class="form-item-view">@Html.Raw(fieldModel.merk)</div>
                  </div>

                    <div id="form_item_jumlah_zona" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "jumlah_zona")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_jumlah_zona" class="form-item-view">@Html.Raw(fieldModel.jumlah_zona)</div>
                  </div>

                    <div id="form_item_deskripsi" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "deskripsi")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_deskripsi" class="form-item-view">@Html.Raw(fieldModel.deskripsi)</div>
                  </div>

                    <div id="form_item_photo" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "photo")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_photo" class="form-item-view">@Html.Raw(fieldModel.photo)</div>
                  </div>

                    <div id="form_item_tgl_periksa_akhir" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "tgl_periksa_akhir")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_tgl_periksa_akhir" class="form-item-view">@Html.Raw(fieldModel.tgl_periksa_akhir)</div>
                  </div>

                    <div id="form_item_hasil_periksa_akhir" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_fas_spek_alarm_kebakaran", "hasil_periksa_akhir")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_hasil_periksa_akhir" class="form-item-view">@Html.Raw(fieldModel.hasil_periksa_akhir_text)</div>
                  </div>

				</div>
			</div>
		</div>
		<div class="panel-footer">
			<a id="close" class="k-button k-button-icontext" href="#"><i class="far fa-times-circle"></i>&nbsp;<span id="btnCloseText">@ResxHelper.GetValue("Message", "Close", "Tutup")</span></a>
		</div>
	</div>
</div>
<script>
    $("#close").kendoButton();
    $("#close").click(function (e) {
        close_self();
    });
    function onCloseDialog(e) { $('#dialog').html(''); }
    function closeDialog() { $("#dialog").data("kendoWindow").close(); }
    function close_self() { if (window.parent.$("#dialog_ta_fas_spek_alarm_kebakaran").data("kendoWindow")) { window.parent.$("#dialog_ta_fas_spek_alarm_kebakaran").data("kendoWindow").close(); } else { window.parent.$("#dialog").data("kendoWindow").close(); } }
</script>