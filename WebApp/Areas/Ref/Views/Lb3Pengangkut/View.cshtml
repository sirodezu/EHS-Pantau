                  </div>

                    <div id="form_item_tgl_berlaku_izin_dishub" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ref_lb3_pengangkut", "tgl_berlaku_izin_dishub")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_tgl_berlaku_izin_dishub" class="form-item-view">@Html.Raw(fieldModel.tgl_berlaku_izin_dishub)</div>
                  </div>

                    <div id="form_item_masa_berlaku_izin_dishub" class="form-group">
