                           <div class="form-item-label220">@ResxHelper.GetValue("ta_lb3", "jenis_limbah_dihasilkan_nama")<span id="label_req_jenis_limbah_dihasilkan_nama" class="required"> *</span></div>
                           <div class="form-item-separator">:</div>
                           <div id="field_item_jenis_limbah_dihasilkan_nama" class="form-item-field">
