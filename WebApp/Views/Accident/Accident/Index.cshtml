        var dampak_fasilitas_id_ds = new kendo.data.DataSource({ transport: { read: { url: "@ViewBag.BaseUrl/Ref/LiteralData/LookupData", dataType: "json", data: dampak_fasilitas_id_param, type: "POST" } } });
