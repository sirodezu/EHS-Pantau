@using WebApp
@using Microsoft.AspNetCore.Http;
@{
    Layout = "~/Views/Shared/_LayoutNoHeader.cshtml";
    var fieldModel = ViewData["fieldModel"] as WebApp.Models.AuditKriteriaCsmsModel.ViewModel;
	ViewData["Title"] = ViewData["TitleHeader"];
    ViewData["PageName"] = "auditkriteriacsms";
    ViewData["Heading"] = ViewData["TitleHeader"];
    ViewData["Category1"] = "";
    ViewData["Category2"] = "";
}
<div class="panel panel-form">
	<div class="panel-container">
		<div class="panel-heading">
			<div class="title-header">@ViewData["TitleHeader"]</div>
		</div>
		<div class="panel-content">
			<div class="row">
                <div class="col-lg-5">

                    <div id="form_item_id" class="form-group-none">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_audit_kriteria_csms", "id")</div>
                        <div class="form-item-separator">:</div>
                        <div id="view_item_id" class="form-item-view">@Html.Raw(fieldModel.id)</div>
                    </div>

                    <div id="form_item_audit_id" class="form-group-none">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_audit_kriteria_csms", "audit_id")</div>
                        <div class="form-item-separator">:</div>
                        <div id="view_item_audit_id" class="form-item-view">@Html.Raw(fieldModel.audit_id)</div>
                    </div>

                    <div id="form_item_kriteria_audit_id" class="form-group-none">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_audit_kriteria_csms", "kriteria_audit_id")</div>
