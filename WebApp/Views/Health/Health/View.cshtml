                    </div>

                </div>
                <div class="col-lg-6">

                    <div id="form_item_deskripsi_diderita" class="form-group">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_health", "deskripsi_diderita")</div>
                        <div class="form-item-separator">:</div>
                        <div id="view_item_deskripsi_diderita" class="form-item-view">@Html.Raw(fieldModel.deskripsi_diderita)</div>
                    </div>

                    <div id="form_item_deskripsi_pengobatan" class="form-group">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_health", "deskripsi_pengobatan")</div>
