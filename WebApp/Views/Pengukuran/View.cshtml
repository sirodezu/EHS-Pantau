                    <div id="form_item_file_path_hasil_uji" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "file_path_hasil_uji")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_file_path_hasil_uji" class="form-item-view">@Html.Raw(fieldModel.file_path_hasil_uji)</div>
                  </div>

                    <div id="form_item_file_path_baku_mutu" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "file_path_baku_mutu")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_file_path_baku_mutu" class="form-item-view">@Html.Raw(fieldModel.file_path_baku_mutu)</div>
                  </div>

                    <div id="form_item_keterangan_uji" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_pengukuran", "keterangan_uji")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_keterangan_uji" class="form-item-view">@Html.Raw(fieldModel.keterangan_uji)</div>
                  </div>

				</div>
			</div>
		</div>
		<div class="panel-footer">
			<a id="close" class="k-button k-button-icontext" href="#"><i class="far fa-times-circle"></i>&nbsp;<span id="btnCloseText">@ResxHelper.GetValue("Message", "Close", "Tutup")</span></a>
		</div>
	</div>
</div>
<script>
    $("#close").kendoButton();
    $("#close").click(function (e) {
        close_self();
    });
    function onCloseDialog(e) { $('#dialog').html(''); }
    function closeDialog() { $("#dialog").data("kendoWindow").close(); }
    function close_self() { if (window.parent.$("#dialog_ta_pengukuran").data("kendoWindow")) { window.parent.$("#dialog_ta_pengukuran").data("kendoWindow").close(); } else { window.parent.$("#dialog").data("kendoWindow").close(); } }
</script>