                                   var nilai_bendera_id_param = {item : {value:"id",text:"nama"},parent:[{ field: 'cat_id',opr:'=', value: '165' }], order:[]};
                                   var nilai_bendera_id_ds = new kendo.data.DataSource({ transport: { read: { url: "@ViewBag.BaseUrl/Ref/LiteralData/LookupData", dataType: "json", data: nilai_bendera_id_param, type: "POST" } } });
                                   var nilai_bendera_id = $("#nilai_bendera_id").kendoComboBox({ filter: "contains", placeholder: "", dataTextField: "text", dataValueField: "value", dataSource: nilai_bendera_id_ds, change:onChange_nilai_bendera_id }).data("kendoComboBox");
                                   $(document).ready(function () { $("#nilai_bendera_id").data("kendoComboBox").value('@fieldModel.nilai_bendera_id'); });
                                   function onChange_nilai_bendera_id() {if ($("#nilai_bendera_id").data("kendoComboBox").value() != '' && $("#nilai_bendera_id").data("kendoComboBox")._oldIndex == -1) {$("#nilai_bendera_id").data("kendoComboBox").value('');}
