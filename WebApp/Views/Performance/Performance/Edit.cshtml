            //
        } else {
            if (tgl_akhir_sakit < tgl_mulai_sakit) {
                notification.show({ title: "Tanggal Sakit", message: "Tanggal Akhir Sakit harus lebih besar dari Tanggal Awal Sakit" }, "error");
                $("#dt_sakit_tgl_akhir").data("kendoDatePicker").value($("#dt_sakit_tgl_mulai").data("kendoDatePicker").value());
                $("#sakit_tgl_mulai").val(kendo.toString($("#dt_sakit_tgl_mulai").data("kendoDatePicker").value(), "yyyy-MM-dd"));
