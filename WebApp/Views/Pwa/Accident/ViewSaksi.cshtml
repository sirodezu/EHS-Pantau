@using WebApp
@using Microsoft.AspNetCore.Http;
@{
    Layout = "~/Views/Shared/Pwa/_LayoutNoHeader.cshtml";
    var fieldModel = ViewData["fieldModel"] as WebApp.Models.AccidentSaksiModel.ViewModel;
	ViewData["Title"] = ViewData["TitleHeader"];
}
<div class="panel panel-form">
	<div class="panel-container">
		<div class="panel-heading">
			<div class="title-header">@ViewData["TitleHeader"]</div>
		</div>
		<div class="panel-body">
			<div class="row">
				<div class="col-lg-12">

                    <div id="form_item_id" class="form-group-none">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_id" class="form-item-view">@Html.Raw(fieldModel.id)</div>
                  </div>

                    <div id="form_item_acc_id" class="form-group-none">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "acc_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_acc_id" class="form-item-view">@Html.Raw(fieldModel.acc_id)</div>
                  </div>

                    <div id="form_item_ehs_area_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "ehs_area_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_ehs_area_id" class="form-item-view">@Html.Raw(fieldModel.ehs_area_id_text)</div>
                  </div>

                    <div id="form_item_ba_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "ba_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_ba_id" class="form-item-view">@Html.Raw(fieldModel.ba_id_text)</div>
                  </div>

                    <div id="form_item_pa_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "pa_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_pa_id" class="form-item-view">@Html.Raw(fieldModel.pa_id_text)</div>
                  </div>

                    <div id="form_item_psa_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "psa_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_psa_id" class="form-item-view">@Html.Raw(fieldModel.psa_id_text)</div>
                  </div>

                    <div id="form_item_company_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "company_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_company_id" class="form-item-view">@Html.Raw(fieldModel.company_id_text)</div>
                  </div>

                    <div id="form_item_person_id" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "person_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_person_id" class="form-item-view">@Html.Raw(fieldModel.person_id_text)</div>
                  </div>

                    <div id="form_item_person_jabatan" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "person_jabatan")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_person_jabatan" class="form-item-view">@Html.Raw(fieldModel.person_jabatan)</div>
                  </div>

                    <div id="form_item_deskripsi" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ta_acc_saksi", "deskripsi")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_deskripsi" class="form-item-view">@Html.Raw(fieldModel.deskripsi)</div>
                  </div>

				</div>
			</div>
		</div>
		<div class="panel-footer">
			<a id="close" class="k-button k-button-icontext" href="#"><i class="far fa-times-circle"></i>&nbsp;<span id="btnCloseText">@ResxHelper.GetValue("Message", "Close", "Tutup")</span></a>
		</div>
	</div>
</div>
<script>
    $("#close").kendoButton();
    $("#close").click(function (e) {
        close_self();
    });
    function onCloseDialog(e) { $('#dialog').html(''); }
    function closeDialog() { $("#dialog").data("kendoWindow").close(); }
    function close_self() { if (window.parent.$("#dialog_ta_acc_saksi").data("kendoWindow")) { window.parent.$("#dialog_ta_acc_saksi").data("kendoWindow").close(); } else { window.parent.$("#dialog").data("kendoWindow").close(); } }
</script>