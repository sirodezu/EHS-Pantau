        function draw_result_sp(data, result_id) {
            if (data != null && data != undefined && data.length > 0) {
                jQuery.each(data, function (i, val) {
                    if (val.id == '4') { $('#sp'+result_id+'1').text(val.accidentCount); }
                    if (val.id == '3') { $('#sp'+result_id+'2').text(val.accidentCount); }
                    if (val.id == '1') { $('#sp'+result_id+'3').text(val.accidentCount); }
                    if (val.id == '8') { $('#sp'+result_id+'4').text(val.accidentCount); }
                    if (val.id == '567') { $('#sp'+result_id+'5').text(val.accidentCount); }
                });
            }
            
