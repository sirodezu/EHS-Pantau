                        <div class="form-item-label220">@ResxHelper.GetValue("ta_audit_kriteria_csms", "lampiran_file_path")</div>
                        <div class="form-item-separator">:</div>
                        <div id="field_item_lampiran_file_path" class="form-item-field">
                            <input name="lampiran_file_path_file" id="lampiran_file_path_file" type="file" />
                            <input name="lampiran_file_path" id="lampiran_file_path" type="hidden" value="@fieldModel.lampiran_file_path" data-required-msg="@ResxHelper.GetValue("Message", "RequiredTextFormat").Replace("{0}", @ResxHelper.GetValue("ta_audit_kriteria_csms", "lampiran_file_path"))" />
                            <script>
                                var lampiran_file_path_init = @Html.Raw(fieldModel.lampiran_file_path_init);
                                $("#lampiran_file_path_file").kendoUpload({multiple:true,
                                    async: {
                                        saveUrl: "@ViewBag.BaseUrl/AuditKriteriaCsms/save_lampiran_file_path",
                                        removeUrl: "@ViewBag.BaseUrl/AuditKriteriaCsms/remove_lampiran_file_path",
                                        autoUpload: true
                                    },
                                    validation: {allowedExtensions: [".jpg", ".jpeg", ".png", ".pdf", ".xls", ".xlsx"], maxFileSize: 2097152},
                                    files: lampiran_file_path_init,
                                    success: onSuccessUpload,
                                    select: OnselectUpload_lampiran_file_path
