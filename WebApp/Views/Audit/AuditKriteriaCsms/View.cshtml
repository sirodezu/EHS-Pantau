                        <div id="view_item_csms_resiko_id" class="form-item-view">@Html.Raw(fieldModel.csms_resiko_id_text)</div>
                    </div>




                    <div><p>&nbsp;</p></div>


                    <table class="table" width="80%">
                        <thead>
                            <tr>
                                <th nowrap scope="col">Title</th>
                                <th scope="col">Actual</th>
                                <th scope="col">Bobot (%)</th>
                                <th scope="col">Pemenuhan</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr>
                                <td scope="row"><b>@ResxHelper.GetValue("ta_audit_kriteria_csms", "green_strategy")</b></td>
                                <td>
                                    <div id="view_item_green_strategy_actual" class="form-item-view">@Html.Raw(fieldModel.green_strategy_actual)</div>
                                </td>
                                <td>
                                    <div id="view_item_green_strategy_bobot" class="form-item-view">@Html.Raw(fieldModel.green_strategy_bobot)</div>
                                </td>
                                <td>
                                    <div id="view_item_green_strategy_penuh" class="form-item-view">@Html.Raw(fieldModel.green_strategy_penuh)</div>
                                </td>
                            </tr>
                            <tr>
                                <td scope="row"><b>@ResxHelper.GetValue("ta_audit_kriteria_csms", "green_process")</b></td>
                                <td>
                                    <div id="view_item_green_process_actual" class="form-item-view">@Html.Raw(fieldModel.green_process_actual)</div>
                                </td>
                                <td>
                                    <div id="view_item_green_process_bobot" class="form-item-view">@Html.Raw(fieldModel.green_process_bobot)</div>
                                </td>
                                <td>
                                    <div id="view_item_green_process_penuh" class="form-item-view">@Html.Raw(fieldModel.green_process_penuh)</div>
                                </td>
                            </tr>
                            <tr>
                                <td scope="row"><b>@ResxHelper.GetValue("ta_audit_kriteria_csms", "green_product")</b></td>
                                <td>
