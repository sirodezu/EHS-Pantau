            $("#psa_id").data("kendoComboBox").dataSource.read(psa_id_param);
        }


        //=== filter company_id 
        var company_id_param = { item: { value: 'id', text: 'kode,nama', distinct: '0' } ,parent:[], order:[]};
        var company_id_ds = new kendo.data.DataSource({ transport: { read: { url: "@ViewBag.BaseUrl/Ref/Company/LookupData", dataType: "json", data: company_id_param, type: "POST" } } });
        var company_id = $("#company_id").kendoComboBox({ filter: "contains", placeholder: "", dataTextField: "text", dataValueField: "value", change: onChange_company_id, dataSource: company_id_ds }).data("kendoComboBox");
        $(document).ready(function () { $("#company_id").data("kendoComboBox").value('@filterColumn.company_id'); });
        function onChange_company_id() {
            if ($("#company_id").data("kendoComboBox").value() != '' && $("#company_id").data("kendoComboBox")._oldIndex == -1) { $("#company_id").data("kendoComboBox").value(''); }
            onFilterHeader_ta_acc();
        }
        

        //=== filter jenis_kejadian_id 
        var jenis_kejadian_id_param = { item: { value: 'id', text: 'nama', distinct: '0' } ,parent:[{ field: 'cat_id',opr:'=', value: '141' }], order:[]};
        var jenis_kejadian_id_ds = new kendo.data.DataSource({ transport: { read: { url: "@ViewBag.BaseUrl/Ref/LiteralData/LookupData", dataType: "json", data: jenis_kejadian_id_param, type: "POST" } } });
        var jenis_kejadian_id = $("#jenis_kejadian_id").kendoComboBox({ filter: "contains", placeholder: "", dataTextField: "text", dataValueField: "value", change: onChange_jenis_kejadian_id, dataSource: jenis_kejadian_id_ds }).data("kendoComboBox");
        $(document).ready(function () { $("#jenis_kejadian_id").data("kendoComboBox").value('@filterColumn.jenis_kejadian_id'); });
        function onChange_jenis_kejadian_id() {
            if ($("#jenis_kejadian_id").data("kendoComboBox").value() != '' && $("#jenis_kejadian_id").data("kendoComboBox")._oldIndex == -1) { $("#jenis_kejadian_id").data("kendoComboBox").value(''); }
            onFilterHeader_ta_acc();
        }
        

        //=== filter lokasi_kejadian_id 
        var lokasi_kejadian_id_param = { item: { value: 'id', text: 'nama', distinct: '0' } ,parent:[{ field: 'cat_id',opr:'=', value: '31' }], order:[]};
        var lokasi_kejadian_id_ds = new kendo.data.DataSource({ transport: { read: { url: "@ViewBag.BaseUrl/Ref/LiteralData/LookupData", dataType: "json", data: lokasi_kejadian_id_param, type: "POST" } } });
