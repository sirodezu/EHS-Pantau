                        <div id="view_item_csms_kategori_id" class="form-item-view">@Html.Raw(fieldModel.csms_kategori_id_text)</div>
                    </div>

                    <div id="form_item_csms_tahapan_id" class="form-group">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_audit_kriteria_csms", "csms_tahapan_id")</div>
                        <div class="form-item-separator">:</div>
                        <div id="view_item_csms_tahapan_id" class="form-item-view">@Html.Raw(fieldModel.csms_tahapan_id_text)</div>
                    </div>

                    <div id="form_item_nama_vendor" class="form-group">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_audit_kriteria_csms", "nama_vendor")</div>
                        <div class="form-item-separator">:</div>
                        <div id="view_item_nama_vendor" class="form-item-view">@Html.Raw(fieldModel.nama_vendor)</div>
