                        <div class="form-item-label220">@ResxHelper.GetValue("ta_health", "kategori_health_colour_id")</div>
                        <div class="form-item-separator">:</div>
                        <div id="view_item_kategori_health_colour_id" class="form-item-view">@Html.Raw(fieldModel.kategori_health_colour_id_text)</div>
                    </div>

                    <div id="form_item_photo_file_name" class="form-group-none">
                        <div class="form-item-label220">@ResxHelper.GetValue("ta_health", "photo_file_name")</div>
                        <div class="form-item-separator">:</div>
