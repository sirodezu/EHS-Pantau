                      <div class="form-item-separator">:</div>
                      <div id="view_item_tgl_berlaku_izin_klh" class="form-item-view">@Html.Raw(fieldModel.tgl_berlaku_izin_klh)</div>
                  </div>

                    <div id="form_item_masa_berlaku_izin_klh" class="form-group">
                      <div class="form-item-label">@ResxHelper.GetValue("ref_lb3_pengangkut", "masa_berlaku_izin_klh")</div>
