@using WebApp
@using Microsoft.AspNetCore.Http;
@{
    Layout = "~/Views/Shared/_LayoutNoHeader.cshtml";
    var fieldModel = ViewData["fieldModel"] as WebApp.Models.Nonlb3Model.ViewModel;
	ViewData["Title"] = ViewData["TitleHeader"];
    ViewData["PageName"] = "nonlb3";
    ViewData["Heading"] = ViewData["TitleHeader"];
    ViewData["Category1"] = "";
    ViewData["Category2"] = "";
}
<div class="panel panel-form">
	<div class="panel-container">
		<div class="panel-heading">
			<div class="title-header">@ViewData["TitleHeader"]</div>
		</div>
		<div class="panel-content">
			<div class="row">
				<div class="col-lg-12">

                    <div id="form_item_id" class="form-group-none">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_id" class="form-item-view">@Html.Raw(fieldModel.id)</div>
                  </div>

                    <div id="form_item_ehs_area_id" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "ehs_area_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_ehs_area_id" class="form-item-view">@Html.Raw(fieldModel.ehs_area_id_text)</div>
                  </div>

                    <div id="form_item_ba_id" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "ba_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_ba_id" class="form-item-view">@Html.Raw(fieldModel.ba_id_text)</div>
                  </div>

                    <div id="form_item_pa_id" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "pa_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_pa_id" class="form-item-view">@Html.Raw(fieldModel.pa_id_text)</div>
                  </div>

                    <div id="form_item_psa_id" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "psa_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_psa_id" class="form-item-view">@Html.Raw(fieldModel.psa_id_text)</div>
                  </div>

                    <div id="form_item_bulan" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "bulan")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_bulan" class="form-item-view">@Html.Raw(fieldModel.bulan_text)</div>
                  </div>

                    <div id="form_item_tahun" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "tahun")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_tahun" class="form-item-view">@Html.Raw(fieldModel.tahun_text)</div>
                  </div>

                    <div id="form_item_jenis_limbah_id" class="form-group-none">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "jenis_limbah_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_jenis_limbah_id" class="form-item-view">@Html.Raw(fieldModel.jenis_limbah_id_text)</div>
                  </div>

                    <div id="form_item_timbulan_limbah_cair" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "timbulan_limbah_cair")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_timbulan_limbah_cair" class="form-item-view">@Html.Raw(fieldModel.timbulan_limbah_cair)</div>
                  </div>

                    <div id="form_item_timbulan_limbah_padat" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "timbulan_limbah_padat")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_timbulan_limbah_padat" class="form-item-view">@Html.Raw(fieldModel.timbulan_limbah_padat)</div>
                  </div>

                    <div id="form_item_deskripsi_limbah" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "deskripsi_limbah")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_deskripsi_limbah" class="form-item-view">@Html.Raw(fieldModel.deskripsi_limbah)</div>
                  </div>

                    <div id="form_item_pengelolaan_oleh_id" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "pengelolaan_oleh_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_pengelolaan_oleh_id" class="form-item-view">@Html.Raw(fieldModel.pengelolaan_oleh_id_text)</div>
                  </div>

                    <div id="form_item_usaha_kurang_limbah_id" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "usaha_kurang_limbah_id")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_usaha_kurang_limbah_id" class="form-item-view">@Html.Raw(fieldModel.usaha_kurang_limbah_id_text)</div>
                  </div>

                    <div id="form_item_deskripsi_usaha" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "deskripsi_usaha")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_deskripsi_usaha" class="form-item-view">@Html.Raw(fieldModel.deskripsi_usaha)</div>
                  </div>

                    <div id="form_item_deskripsi_usaha_file_path" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "deskripsi_usaha_file_path")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_deskripsi_usaha_file_path" class="form-item-view">@Html.Raw(fieldModel.deskripsi_usaha_file_path)</div>
                  </div>

                    <div id="form_item_usaha_kurang_limbah_m3" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "usaha_kurang_limbah_m3")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_usaha_kurang_limbah_m3" class="form-item-view">@Html.Raw(fieldModel.usaha_kurang_limbah_m3)</div>
                  </div>

                    <div id="form_item_usaha_kurang_limbah_kg" class="form-group">
                      <div class="form-item-label220">@ResxHelper.GetValue("ta_nonlb3", "usaha_kurang_limbah_kg")</div>
                      <div class="form-item-separator">:</div>
                      <div id="view_item_usaha_kurang_limbah_kg" class="form-item-view">@Html.Raw(fieldModel.usaha_kurang_limbah_kg)</div>
                  </div>

				</div>
			</div>
		</div>
		<div class="panel-footer">
			<a id="close" class="k-button k-button-icontext" href="#"><i class="far fa-times-circle"></i>&nbsp;<span id="btnCloseText">@ResxHelper.GetValue("Message", "Close", "Tutup")</span></a>
		</div>
	</div>
</div>
<script>
    $("#close").kendoButton();
    $("#close").click(function (e) {
        close_self();
    });
    function onCloseDialog(e) { $('#dialog').html(''); }
    function closeDialog() { $("#dialog").data("kendoWindow").close(); }
    function close_self() { if (window.parent.$("#dialog_ta_nonlb3").data("kendoWindow")) { window.parent.$("#dialog_ta_nonlb3").data("kendoWindow").close(); } else { window.parent.$("#dialog").data("kendoWindow").close(); } }

    function display_by_type() {
        var isCair = 0;
        var isPadat = 0;
        var dataItem = [@Html.Raw(fieldModel.jenis_limbah_id)];
        if (dataItem == '1') { isCair = 1; }
        if (dataItem == '2' || dataItem == '3') { isPadat = 1; }
        
        if (isCair == 1) {
            $('#form_item_timbulan_limbah_cair').show();
            $('#form_item_usaha_kurang_limbah_m3').show();
        } else {
            $('#form_item_timbulan_limbah_cair').hide();
            $('#form_item_usaha_kurang_limbah_m3').hide();
        }
        if (isPadat == 1) {
            $('#form_item_timbulan_limbah_padat').show();
            $('#form_item_usaha_kurang_limbah_kg').show();
        } else {
            $('#form_item_timbulan_limbah_padat').hide();
            $('#form_item_usaha_kurang_limbah_kg').hide();
        }
    }	
    $(document).ready(function () {
        display_by_type()
    });		
</script>